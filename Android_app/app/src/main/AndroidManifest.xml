        <meta-data
            android:name="com.google.android.geo.AIzaSyB0EFKRAIwdLx_95aI4vWzpe0RAdM6wMqU"
            android:value="@string/google_maps_key" />

        <activity android:name=".RestaurantsMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>